generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model tpub_categoria {
  id_cat  Int      @id @unique @default(autoincrement())
  pk_giro Int
  nombre  String?
  imagen  String?
  b_cat   Boolean? @default(true)
  n_orden Int
}